{
  "name": "vimflowy",
  "version": "0.2.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "cross-env": "^7.0.3",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.4",
    "file-saver": "^2.0.5",
    "firebase": "^8.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "katex": "^0.12.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "pg": "^8.8.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "react": "^17.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "ws": "^6.1.2"
  },
  "scripts": {
    "lint": "tslint --exclude 'node_modules/**/*' '**/*.ts' '**/*.tsx'",
    "startprod": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' server/prod.ts",
    "typecheck": "tsc -p . --noEmit",
    "verify": "npm run lint && npm run typecheck && npm test",
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --opts test/mocha.opts",
    "test-windows": "SET TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}&& mocha --opts test/mocha.opts",
    "watchtest": "npm test -- --reporter dot --watch",
    "profiletest": "npm test -- --prof",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others \"cross-env BROWSER=none npm run startprod\" \"wait-on tcp:3000 && npm run electron\"",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@types/better-sqlite3": "^7.6.1",
    "@types/express": "^4.16.0",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.19",
    "@types/jquery": "^3.5.5",
    "@types/katex": "^0.11.0",
    "@types/lodash": "^4.14.166",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^12.19.11",
    "@types/pg": "^8.6.5",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.10",
    "@types/react": "^16.14.2",
    "@types/ws": "0.0.41",
    "concurrently": "^7.4.0",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron": "^6.1.12",
    "ignore-styles": "^5.0.1",
    "minimist": "^1.2.2",
    "mocha": "^10.0.0",
    "sass": "^1.55.0",
    "tslint": "^6.1.3",
    "wait-on": "^6.0.1"
  },
  "main": "public/electron.js",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vimflowy"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
